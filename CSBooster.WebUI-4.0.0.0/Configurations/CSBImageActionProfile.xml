      <fileNamePostExtension></fileNamePostExtension>
      <format>jpg</format>
      <resolution>72</resolution>
      <quality>4</quality>
      <action ID="crop">
         <bestFit>true</bestFit>
         <bestFitWidth>100</bestFitWidth>
         <bestFitHeight>200</bestFitHeight>
         <left>50</left>
         <right>50</right>
         <top>50</top>
         <bottom>50</bottom>
      </action>
      <action ID="resizetomax">
         <interpolation>bicubic</interpolation>
         <maxwidth>570</maxwidth>
         <maxheight>0</maxheight>

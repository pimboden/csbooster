<?xml version="1.0" encoding="utf-8" ?>
<root>
    <actionProfile ID="ExtraSmallJpg">
        <ApplicationURL></ApplicationURL>
        <autoSave>true</autoSave>
        <VirtualOutPath>##MEDIASERVER##/##FOLDERID##\P\XS</VirtualOutPath>
        <fileOutPath>##MEDIAPATH##\MEDIA\##FOLDERID##\P\XS</fileOutPath>
        <fileNamePreExtension></fileNamePreExtension>
        <fileNamePostExtension></fileNamePostExtension>
        <format>jpg</format>
        <resolution>72</resolution>
        <quality>12</quality>
        <action ID="resizetomax">
            <interpolation>bicubic</interpolation>
            <maxwidth>60</maxwidth>
            <maxheight>60</maxheight>
        </action>
    </actionProfile>
